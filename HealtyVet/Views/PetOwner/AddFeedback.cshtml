@model HealtyVet.Models.PetOwnerModel.AddFeedbackViewModel
@{
    ViewData["Title"] = "AddFeedback";
}
<form asp-action="AddFeedback" method="post">

    <div class="row">
        <div class="col-2">
         Adauga comentariu:
        </div>
        <div class="col-10">
            <input class="form-control" type="text" asp-for="Description" />
        </div>
    </div>
   
     

    <div class="row">
        <div class="col-10"></div>
        <div class="col-2">
            <button type="submit" class="btn btn-outline-success">Salveaza</button>
        </div>
    </div>
</form>
